<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="text/x-deck">
     <comment>Deck</comment>
     <glob pattern="*.deck"/>
   </mime-type>
   <mime-type type="image/x-deck-image">
     <comment>Deck Image</comment>
     <sub-class-of type="image/jpg"/>
     <glob pattern="*.deck.jpg"/>
   </mime-type>
</mime-info>
